{
	"version": "2.0",
	"extensions": {
		"queues": {
			"batchSize": 16,
			"maxDequeueCount": 5,
			"visibilityTimeout": "00:00:30" // Time to wait before making a message visible again after a failed attempt
		}
	},
	"logging": {
		"applicationInsights": {
			"samplingSettings": {
				"isEnabled": true,
				"excludedTypes": "Request"
			},
			"enableLiveMetricsFilters": true
		}
	}
}
